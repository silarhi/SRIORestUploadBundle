{
    "name": "srio/rest-upload-bundle",
    "description": "Handle multiple rest upload ways",
    "keywords": ["symfony", "file", "upload", "api", "rest", "multipart", "resumable", "form-data"],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Samuel ROZE",
            "email": "samuel@sroze.io",
            "homepage": "http://www.sroze.io"
        }
    ],
    "require": {
        "php": "^8.2"
    },
    "require-dev": {
        "doctrine/doctrine-bundle": "^2.10",
        "knplabs/knp-gaufrette-bundle": "^0.8",
        "oneup/flysystem-bundle": "^4.9",
        "doctrine/orm": "^2.16",
        "symfony/framework-bundle": "^6.2",
        "symfony/console": "^6.2",
        "symfony/form": "^6.2",
        "symfony/validator": "^6.2",
        "symfony/yaml": "^6.3",
        "symfony/browser-kit": "^6.3",
        "doctrine/annotations": "^2.0",
        "phpunit/phpunit": "^10.4",
        "rector/rector": "^0.18.6",
        "vimeo/psalm": "^5.15",
        "phpstan/phpstan": "^1.10"
    },
    "autoload": {
        "psr-4": { "SRIO\\RestUploadBundle\\": "src/" }
    },
    "autoload-dev": {
        "psr-4": {
            "SRIO\\RestUploadBundle\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./test.sh"
    },
    "target-dir": "SRIO/RestUploadBundle",
    "config": {
    }
}
